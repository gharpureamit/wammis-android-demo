<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/header_cover_image"
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:scaleType="centerCrop"
            android:src="@drawable/navigation_header_image" />

        <RelativeLayout
            android:id="@+id/profile_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/profile_circular_border_imageview"
            android:layout_marginTop="40dp"
            android:elevation="4dp"
            android:paddingBottom="24dp">


            <TextView
                android:id="@+id/user_profile_name"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="25dp"
                android:text="@string/drawer_username"
                android:textColor="#fff"
                android:textSize="24sp"
                android:textStyle="bold"
                android:textAlignment="center" />

        </RelativeLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/header_cover_image"
            android:layout_marginTop="65dp"
            android:orientation="vertical">

            <TableLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/tableLayout11"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:shrinkColumns="*"
                android:stretchColumns="*"
                android:layout_gravity="center"
                android:layout_marginRight="20dp"
                android:layout_marginLeft="13dp"
                >

                <TableRow
                    android:id="@+id/tableRow111"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:background="@drawable/cell_shape_elig_header">
                    <TextView
                        android:id="@+id/TextView0041"
                        android:textSize="10dp"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:layout_weight="0"
                        android:textAlignment="center"
                        android:layout_column="0"></TextView>
                    <TextView
                        android:id="@+id/TextView041"
                        android:text="CLIENT ID     "
                        android:textSize="10dp"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="center"
                        android:layout_column="1"></TextView>
                    <TextView
                        android:id="@+id/TextView0411"
                        android:text="CLIENT NAME"
                        android:textSize="10dp"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="center"
                        android:layout_column="2"></TextView>

                </TableRow>

                <TableRow
                    android:id="@+id/tableRow2"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:background="@drawable/cell_shape_elig_content">
                    <CheckBox
                        android:layout_width="5dp"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/PBox1"
                        android:layout_column="0" />

                    <TextView
                        android:id="@+id/TextView0412"
                        android:text="@string/client_id"
                        android:textSize="10dp"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="center"
                        android:layout_column="1"></TextView>
                    <TextView
                        android:id="@+id/CName1"
                        android:text="@string/drawer_username"
                        android:textSize="10dp"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="center"
                        android:layout_column="2"></TextView>

                </TableRow>
                <TableRow
                    android:id="@+id/tableRow3"
                    android:background="@drawable/cell_shape_elig_content">
                    <CheckBox
                        android:layout_width="20dp"
                        android:layout_height="40dp"
                        android:text=""
                        android:id="@+id/PBox2"
                        android:layout_column="0" />

                    <TextView
                        android:id="@+id/TextView0413"
                        android:text="@string/dependent_id"
                        android:textSize="10dp"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="center"
                        android:layout_column="1"></TextView>
                    <TextView
                        android:id="@+id/CName2"
                        android:text="@string/dependent_name"
                        android:textSize="10dp"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="center"
                        android:layout_column="2"></TextView>
                </TableRow>
            </TableLayout>

            <TableLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/PlanDetailsLayout1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding ="5dp"
                android:shrinkColumns="*"
                android:stretchColumns="*"
                android:layout_gravity="center"
                android:layout_marginRight="20dp"
                android:layout_marginLeft="10dp"
                >


                <TableRow
                    android:id="@+id/tableRows111"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="40dp"
                    android:background="@drawable/cell_shape_elig_header">
                    <TextView
                        android:id="@+id/TextView41"
                        android:text=""
                        android:textSize="10dp"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="0"></TextView>
                    <TextView
                        android:id="@+id/Plan"
                        android:text="PLAN"
                        android:textSize="10dp"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="1"></TextView>
                    <TextView
                        android:id="@+id/PhoneNumber"
                        android:text="PHONE NUMBER"
                        android:textSize="10dp"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="2"></TextView>
                </TableRow>
                <TableRow
                    android:background="@drawable/cell_shape_elig_content">
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/checkBoxPlan1"
                        android:layout_column="0" />

                    <TextView
                        android:id="@+id/PName1"
                        android:text="AmeriGroup Washington Inc"
                        android:textSize="10dp"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="1"></TextView>
                    <TextView
                        android:id="@+id/PNum1"
                        android:text="(800)-123-1234"
                        android:textSize="10dp"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="2"></TextView>
                </TableRow>
                <TableRow android:background="@drawable/cell_shape_elig_content">
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/checkBoxPlan2"
                        android:layout_column="0" />

                    <TextView
                        android:id="@+id/PName2"
                        android:text="Coordinated Care of Washington"
                        android:textSize="10dp"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="1"></TextView>
                    <TextView
                        android:id="@+id/PNum2"
                        android:text="(800)-321-4321"
                        android:textSize="10dp"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="2"></TextView>
                </TableRow>
                <TableRow
                    android:background="@drawable/cell_shape_elig_content">
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/checkBoxPlan3"
                        android:layout_column="0" />

                    <TextView
                        android:id="@+id/PName3"
                        android:text="Community Plan of Washington"
                        android:textSize="10dp"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="1"></TextView>
                    <TextView
                        android:id="@+id/PNum3"
                        android:text="(800)-678-8765"
                        android:textSize="10dp"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="2"></TextView>
                </TableRow>
            </TableLayout>

            <TableLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/PlanDetailsLayout2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding ="5dp"
                android:shrinkColumns="*"
                android:stretchColumns="*"
                android:layout_gravity="center"
                android:layout_marginRight="10dp"
                android:layout_marginLeft="10dp"
                >


                <TableRow
                    android:id="@+id/tableRows1111"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:layout_marginTop="40dp"
                    android:background="@drawable/cell_shape_elig_header">
                    <TextView
                        android:id="@+id/TextView411"
                        android:text=""
                        android:textSize="10dp"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="0"></TextView>
                    <TextView
                        android:id="@+id/Plan1"
                        android:text="PLAN"
                        android:textSize="10dp"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="1"></TextView>
                    <TextView
                        android:id="@+id/PhoneNumber1"
                        android:text="PHONE NUMBER"
                        android:textSize="10dp"
                        android:textStyle="bold"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="2"></TextView>
                </TableRow>
                <TableRow
                    android:background="@drawable/cell_shape_elig_content">
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/checkBoxPlan11"
                        android:layout_column="0" />

                    <TextView
                        android:id="@+id/PName11"
                        android:text="AmeriGroup Washington Inc"
                        android:textSize="10dp"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="1"></TextView>
                    <TextView
                        android:id="@+id/PNum11"
                        android:text="(800)-123-1234"
                        android:textSize="10dp"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="2"></TextView>
                </TableRow>
                <TableRow
                    android:background="@drawable/cell_shape_elig_content">
                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:id="@+id/checkBoxPlan21"
                        android:layout_column="0" />

                    <TextView
                        android:id="@+id/PName21"
                        android:text="Molina HealthCare of Washington"
                        android:textSize="10dp"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="1"></TextView>
                    <TextView
                        android:id="@+id/PNum21"
                        android:text="(800)-321-4321"
                        android:textSize="10dp"
                        android:typeface="serif"
                        android:layout_weight="1"
                        android:textAlignment="viewStart"
                        android:layout_column="2"></TextView>
                </TableRow>

            </TableLayout>
            <Button
                android:id = "@+id/new_plan_button"
                android:layout_width="200dp"
                android:layout_height="60dp"
                android:layout_margin="25dp"
                android:background="@drawable/app_button_background"
                android:textColor="@color/cardview_light_background"
                android:textAlignment="center"
                android:layout_centerHorizontal="true"
                android:clickable="true"
                android:layout_gravity="center_horizontal"
                android:padding="20dp"
                android:text="Change Plan" />

        </LinearLayout>

    </RelativeLayout>
</ScrollView>